{
	"relative": true,
	"groups": [
		{
			"namespace": "uglified",
			"source": "/templates/",
			"output": "/output/uglified.js",
			"templates": [
				"layout",
				"root",
				"level1/root",
				"level1/level2/root",
				"level1/level2/level3/root"
			]
		},
		{
			"uglify": false,
			"namespace": "templates",
			"source": "/templates/",
			"output": "/output/templates.js",
			"templates": [
				"layout",
				"root",
				"level1/root",
				"level1/level2/root",
				"level1/level2/level3/root"
			]
		},
		{
			"uglify": false,
			"namespace": "very.long.namespace",
			"source": "/templates/",
			"output": "/output/long-namespace.js",
			"templates": [
				"layout",
				"root",
				"level1/root",
				"level1/level2/root",
				"level1/level2/level3/root"
			]
		},
		{
			"uglify": false,
			"namespace": "single.level",
			"source": "/templates/",
			"output": "/output/single-level.js",
			"templates": [
				"layout",
				"root"
			]
		}, 
		{
			"uglify": false,
			"namespace": "singleNamespace",
			"source": "/templates/",
			"output": "/output/single-namespace.js",
			"templates": [
				"layout",
				"root"
			]
		},
		{
			"uglify": false,
			"namespace": "debugEnabled",
			"source": "/templates/",
			"compileDebug": true,
			"output": "/output/debug-enabled.js",
			"templates": [
				"layout",
				"root"
			]
		},
		{
			"uglify": false,
			"namespace": "inlineHelpers",
			"source": "/templates/",
			"inline": true,
			"output": "/output/inline-helpers.js",
			"templates": [
				"layout",
				"root"
			]
		},
		{
			"uglify": false,
			"namespace": "noHelpers",
			"source": "/templates/",
			"helpers": false,
			"output": "/output/no-helpers.js",
			"templates": [
				"layout",
				"root"
			]
		}
	]
}